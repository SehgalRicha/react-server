{
  "name": "babel-plugin-react-server",
  "version": "0.7.0-hack2017-3",
  "description": "Babel plugin for React Server transpilation",
  "repository": "redfin/babel-plugin-react-server",
  "author": "Doug Wade <doug.wade@redfin.com>",
  "main": "lib/index.js",
  "dependencies": {
    "react-server-module-tagger": "^0.7.0-hack2017-3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.8.1",
    "eslint-plugin-react": "^6.4.1",
    "mocha": "^3.2.0"
  },
  "scripts": {
    "clean": "rm -rf lib npm-debug.log*",
    "lint": "eslint src/ test/index.js",
    "build": "babel src -d lib",
    "test": "mocha --compilers js:babel-register && npm run lint",
    "test:watch": "npm run test -- --watch",
    "prepublish": "npm run clean && npm run build"
  },
  "publishConfig": {
    "registry": "http://npm.redfintest.com:4873"
  },
  "keywords": [
    "react",
    "server",
    "babel",
    "plugin",
    "babel-plugin"
  ]
}
